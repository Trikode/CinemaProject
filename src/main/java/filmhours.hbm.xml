<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.emit.bean.FHD" table="filmhours">  
    <id name="idFilmHours">  
     	<generator class="native"></generator>  
    </id>  
            
    <many-to-one name="IDFilm" column = "IDFilm" fetch="join" lazy="false"></many-to-one>
    <many-to-one name="IDHour" column = "IDHour" fetch="join" lazy="false"></many-to-one>
    <many-to-one name="IDDate" column = "IDDate" fetch="join" lazy="false"></many-to-one>
  

        
            
  </class>  
            
 </hibernate-mapping> 
